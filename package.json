{
    "name": "@kingga/kc-container",
    "version": "1.0.2",
    "description": "A simple IoC container for the kings collection.",
    "main": "dist/Container.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "nyc mocha 'tests/**/*.ts' --require ts-node/register",
        "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prepublish": "yarn test && yarn prod"
    },
    "keywords": [
        "IoC",
        "Kings Collection",
        "Container",
        "JavaScript",
        "TypeScript",
        "Container"
    ],
    "author": "Isaac Skelton <contact@isaacskelton.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.22",
        "@types/mocha": "^5.2.7",
        "cross-env": "^6.0.3",
        "eslint": "^6.4.0",
        "laravel-mix": "^5.0.0",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "ts-loader": "^6.1.2",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3"
    },
    "dependencies": {
        "camelcase": "^5.3.1"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "src/**/*"
        ],
        "exclude": [
            "**/*/*.d.ts",
            "**/*/*Type.ts"
        ],
        "reporter": [
            "text"
        ],
        "all": true
    }
}
